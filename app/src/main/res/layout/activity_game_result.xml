<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_game"
    tools:context="com.alphago.alphago.activity.GameResultActivity">

    <android.support.constraint.Guideline
        android:id="@+id/rgame_guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.65"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.constraint.Guideline
        android:id="@+id/rgame_guideline2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="232dp" />

    <ImageView
        android:id="@+id/img_rgame_bgqst"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toBottomOf="@+id/rgame_guideline2"
        app:layout_constraintLeft_toRightOf="@+id/layout_game_result"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg_qst2" />

    <ImageView
        android:id="@+id/img_rgame_tch"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/rgame_guideline"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_rgame_result"
        app:srcCompat="@drawable/teacher" />

    <ImageView
        android:id="@+id/img_rgame_result"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_rgame_bgqst"
        app:layout_constraintLeft_toLeftOf="@+id/img_rgame_bgqst"
        app:layout_constraintRight_toRightOf="@+id/img_rgame_bgqst"
        app:layout_constraintTop_toTopOf="@+id/img_rgame_bgqst"
        app:srcCompat="@drawable/bg_result1" />

